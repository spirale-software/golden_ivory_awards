{% extends "admin_layout.html.twig" %}

{% block admin_title %} Détail Nominé {% endblock %}

{% block content %}  
    <div class="well" >
        <div class="row">
            <div class="col-md-6"><h3>Prénom</h3></div>
            <div class="col-md-6"><h4>{{nomine.nom}}</h4></div>           
        </div>

        <div class="row bg-success">
            <div class="col-md-6"><h3>Catégorie</h3></div>
            <div class="col-md-6"><h4>{{nomine.libelleCategorie}}</h4></div>           
        </div>

        <div class="row">
            <div class="col-md-6"><h3>Descriptif</h3></div>
            <div class="col-md-6"><h4>{{nomine.descriptif}}</h4></div>           
        </div>

        <div class="row bg-success">
            <div class="col-md-6"><h3>Actualite</h3></div>
            <div class="col-md-6"><h4>{{nomine.actualite}}</h4></div>           
        </div>
    </div>
    <a class="right" href="{{ path('nomine_all') }}" >retour</a>

{% endblock %}
